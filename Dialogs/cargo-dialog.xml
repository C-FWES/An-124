<group>
<layout>hbox</layout>

<checkbox>
<halign>left</halign>
<label>Locomotive (30 tons, 66138 lbs)</label>
<property>controls/switches/locomotive</property>
<binding>
<command>property-toggle</command>
<property>controls/switches/locomotive</property>
</binding>
</checkbox>

<button>
	<halign>right</halign>
	<legend>Doors</legend>
	<padding>5</padding>
	<binding>
		<command>nasal</command>
		<script>doors.back.toggle();</script>
	</binding>
</button>

<button>
	<halign>right</halign>
	<legend>Load/Unload</legend>
	<padding>5</padding>
	<binding>
		<command>nasal</command>
		<script>cargo.locomotive.toggle();</script>
	</binding>
</button>
</group>

