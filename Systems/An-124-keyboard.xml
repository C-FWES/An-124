<key n="108">
<name>l</name>
<desc>toggle landing lights</desc>
<binding>
<command>nasal</command>
<script>controls.toggleLandingLights();</script>
</binding>
</key>


<key n="76">
<name>L</name>
<desc>toggle landing lights</desc>
<binding>
<command>nasal</command>
<script>controls.toggleExtraLights();</script>
</binding>
</key>

