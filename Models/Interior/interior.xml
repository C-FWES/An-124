<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

<animation>
  <type>rotate</type>
	<condition>
		<less-than-equals>
		<property>sim/model/door-positions/front/position-norm</property>
		<value>10</value>
		</less-than-equals>
	</condition>
  <property>sim/model/door-positions/front/position-norm</property>
 <interpolation>
    <entry>
      <ind>1.0</ind>
      <dep>1.0</dep>
    </entry>
    <entry>
      <ind>0.75</ind>
      <dep>0.5</dep>
    </entry>
    <entry>
      <ind>0.25</ind>
      <dep>0.25</dep>
    </entry>
    <entry>
      <ind>0.0</ind>
      <dep>0.0</dep>
    </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>


<animation>
    <type>material</type>
