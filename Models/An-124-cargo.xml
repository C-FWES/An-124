<?xml version="1.0"?>
<PropertyList>


<!--Locomotive-->

<model>
<name>Locomotive</name>
 <path>Aircraft/An-124/Models/Cargo/Locomotive/ICE-firstcar.xml</path>
 <condition>
 <and>
  <property>controls/switches/locomotive</property>
	<or>
           <less-than>	
	         <property>/velocities/groundspeed-kt</property>
	         <value>1</value>
	         </less-than>
		<greater-than>
		<property>/sim/model/cargo/locomotive/position-norm</property>
		                <value>0.8</value>

		</greater-than>
	</or>
 </and>
 </condition>
<offsets>
            <x-m>50</x-m>
            <y-m>0</y-m>
            <z-m>-5.25</z-m>
            <pitch-deg>0</pitch-deg>
            <heading-deg>0</heading-deg>
        </offsets>
</model>

