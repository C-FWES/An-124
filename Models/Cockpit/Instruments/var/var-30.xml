<PropertyList>
  
  <path>var-30.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    
    <object-name>vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/An-124/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
    <interpolation>
      <entry><ind>  5708.66 </ind><dep>  177.0 </dep></entry>
      <entry><ind>  3937.00 </ind><dep>  142.0 </dep></entry>
      <entry><ind>  1968.50 </ind><dep>   83.0 </dep></entry>
      <entry><ind>   984.25 </ind><dep>   44.0 </dep></entry>
      <entry><ind>     0.00 </ind><dep>    0.0 </dep></entry>
      <entry><ind>  -984.25 </ind><dep>  -44.0 </dep></entry>
      <entry><ind> -1968.50 </ind><dep>  -83.0 </dep></entry>
      <entry><ind> -3937.00 </ind><dep> -142.0 </dep></entry>
      <entry><ind> -5708.66 </ind><dep> -177.0 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
