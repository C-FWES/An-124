<!-- SPEEDBRAKE LEVER -->

<animation>
	<type>pick</type>
	<object-name>speedbrake</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>  
		<binding>
			<command>property-adjust</command>
			<property>controls/flight/speedbrake-lever</property>
			<step>1</step>
			<min>0</min>
			<max>2</max>
		</binding>
		<!--binding>
			<condition>
				<equals>
					<property>/autopilot/autospoilers/step</property>
					<value>3</value>
				</equals>
			</condition>
			<command>property-assign</command>
			<property>/autopilot/autospoilers/step</property>
			<value>0</value>
		</binding-->
	</action>
	<action>
		<button>1</button>
		<repeatable>false</repeatable>  
		<binding>
			<command>property-adjust</command>
			<property>controls/flight/speedbrake-lever</property>
			<step>-1</step>
			<min>0</min>
			<max>2</max>
		</binding>
	</action>
</animation>

<animation>
	<type>rotate</type>
	<object-name>speedbrake</object-name>
	<property>controls/flight/speedbrake-lever</property>
	<!--property>/autopilot/autospoilers/step</property-->
	<interpolation>
		<!--entry>
			<ind>0</ind>
			<dep>-3</dep>
		</entry-->
		<entry>
			<ind>0</ind>
			<dep>0</dep>
		</entry>
		<entry>
			<ind>1</ind>
			<dep>32</dep>
		</entry>
		<entry>
			<ind>2</ind>
			<dep>60</dep>
		</entry>
	</interpolation>
	<factor>60</factor>
	<center>
		<x-m>1.143</x-m>
		<y-m>0</y-m>
		<z-m>0.362</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
	</axis>
</animation>


